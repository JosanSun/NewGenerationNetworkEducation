define("article:widget/dom-ready/dom-ready.js",function(e,n){function t(e){return(parseInt(e,10)+f.length)%f.length}function i(e){if(y[e])return y[e];var n=f.eq(e),t=n.attr("data-text");if(t)return t;var i=n.parents(".exp-content-block");return t=c.trim(c(".exp-content-body",i)[0].innerText),t="END"===t.slice(-3)?t.slice(0,-3):t,y[e]=t,t}function o(e){if(e===p)return l.play(),f.eq(p).addClass("playing"),1;var n=0>p?0:p;f.eq(n).removeClass("playing"),p=t("next"===e?p+1:e),f.eq(p).addClass("playing"),l.changeText(i(p)),l.play()}function a(e){f.eq(t(e)).removeClass("playing"),l.pause()}function d(){if("undefined"==typeof Audio)return 1;l=new u({onplay:function(){r.send(location.href,2019,{module:"article",page:"index",pos:"audio",action:"play"})},onended:function(){f.eq(p).removeClass("playing"),p<f.length-1&&o("next")}}),c.each(f,function(e,n){c(n).attr("data-index",e),c(n).css({display:"inline-block"}),c(n).on("click",function(t){t.preventDefault();parseInt(c(n).attr("data-index"),10);return l.paused||e!==p?(o(e),void r.send(location.href,2019,{module:"article",page:"index",pos:"audio-play",action:"click"})):(a(e),r.send(location.href,2019,{module:"article",page:"index",pos:"audio-pause",action:"click"}),1)})}),s.on("douwan-video/play",function(){a(p)});var e=0;c(".bottom-text-ads").find("#EC_l").find("a").find(".EC_tit").each(function(n,t){"block"===c(t).css("display")&&"hidden"!==c(t).css("visibility")&&e++}),r.send(window.location,2019,{module:"article",page:"index",pos:"bottom",action:"show",adCount:e})}var l,c=e("common-jquery:widget/lib/jquery/jquery.js"),r=e("common-jquery:widget/js/logic/log/log.js"),s=e("common:widget/lib/fis/event/event.js").eventCenter,u=e("common-jquery:widget/js/logic/text2audio/text2audio.es").Text2Audio,p=-1,f=c(".audio-wp"),y=[];n.init=d});